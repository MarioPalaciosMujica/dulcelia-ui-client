   subTitle: 'the bag',\n    image: 'assets/images/slider/27.jpg'\n  }];\n\n  // Categories\n  public categories = ['airbag', 'burn bag', 'briefcase', 'carpet', 'money bag', 'tucker'];\n\n  // Blog\n  public blogs = [{\n    image: 'assets/images/blog/37.jpg',\n    date: '25 January 2018',\n    title: 'Lorem ipsum dolor sit consectetur adipiscing elit,',\n    by: 'John Dio'\n  }, {\n    image: 'assets/images/blog/38.jpg',\n    date: '26 January 2018',\n    title: 'Lorem ipsum dolor sit consectetur adipiscing elit,',\n    by: 'John Dio'\n  }, {\n    image: 'assets/images/blog/39.jpg',\n    date: '27 January 2018',\n    title: 'Lorem ipsum dolor sit consectetur adipiscing elit,',\n    by: 'John Dio'\n  }, {\n    image: 'assets/images/blog/37.jpg',\n    date: '28 January 2018',\n    title: 'Lorem ipsum dolor sit consectetur adipiscing elit,',\n    by: 'John Dio'\n  }];\n\n  ngOnInit(): void {\n    // Change color for this layout\n    document.documentElement.style.setProperty('--theme-deafult', '#f0b54d');\n  }\n\n  ngOnDestroy(): void {\n    // Remove Color\n    document.documentElement.style.removeProperty('--theme-deafult');\n  }\n\n  // Product Tab collection\n  getCollectionProducts(collection) {\n    return this.products.filter((item) => {\n      if (item.collection.find(i => i === collection)) {\n        return item\n      }\n    })\n  }\n\n}\n","<app-header-two [themeLogo]=\"themeLogo\"></app-header-two>\n\n<!-- Home slider start-->\n<section class=\"p-0\">\n  <app-slider \n    [sliders]=\"sliders\" \n    [textClass]=\"'text-center p-center'\"\n    [category]=\"'bags'\"\n    [buttonText]=\"'save up to 50% off'\"> \n  </app-slider>\n</section>\n<!-- Home slider End -->\n\n<!-- Product Tab start-->\n<section class=\"section-b-space\">\n  <div class=\"container\">\n    <div class=\"title2\">\n        <h4>new collection</h4>\n        <h2 class=\"title-inner2\">trending products</h2>\n    </div>\n    <div class=\"row\">\n      <div class=\"col\">\n        <div class=